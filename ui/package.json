    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "watch": "vue-cli-service build --watch"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.0",
    "bootstrap-vue": "^2.0.0-rc.15",
    "c3": "^0.6.13",
    "core-js": "^2.6.5",
    "cronstrue": "^1.65.0",
    "d3": "^5.9.2",
    "downloadjs": "^1.4.7",
    "jquery.flot": "^0.8.3",
    "moment": "^2.24.0",
    "otplib": "^12.0.1",
    "qrcode.vue": "^1.7.0",
    "v-suggestions": "^1.1.1",
    "vue": "^2.6.6",
    "vue-clipboard2": "^0.3.0",
    "vue-good-table": "2.21.1",
    "vue-i18n": "^8.9.0",
    "vue-js-toggle-button": "^1.3.2",
    "vue-resource": "^1.5.1",
    "vue-router": "^3.0.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true,
      "node": true,
      "jquery": true
